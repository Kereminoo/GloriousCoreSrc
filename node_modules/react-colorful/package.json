    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-frame-component": "^5.1.0",
    "size-limit": "^4.10.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3",
    "use-throttled-effect": "0.0.7"
  }
}